{{ header }}
<div id="account-register" class="container">
   <ul class="pts-breadcrumb breadcrumb">
      {% for breadcrumb in breadcrumbs %}
         <li class="breadcrumb-item">
            <a href="{{ breadcrumb.href }}">
               {{ breadcrumb.text }}
            </a>
         </li>
      {% endfor %}
   </ul>
   {% if error_warning %}
      <div class="alert alert-danger alert-dismissible">
         <i class="fa fa-exclamation-circle"></i>
         {{ error_warning }}
      </div>
   {% endif %}
   <div class="pts-row d-flex row">
      {{ column_left }}
      {% if column_left and column_right %}
         {% set class = 'pts-col-sm-6 col-sm-6' %}
         {% set setdivclass = 'pts-col-md-10' %}
      {% elseif column_left or column_right %}
         {% set class = 'pts-col-sm-9 col-sm-9' %}
         {% set setdivclass = 'pts-col-md-8' %}
      {% else %}
         {% set class = 'pts-col-sm-12 col-sm-12' %}
         {% set setdivclass = 'pts-col-md-6' %}
      {% endif %}
      <div id="content" class="{{ class }}">
         {{ content_top }}
         <!-- <h1>{{ heading_title }}</h1> -->
         <!-- <p>{{ text_account_already }}</p> -->
